<!DOCTYPE html>
<html>
  <head>
    <title>Samplify</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag    'desktop' if desktop_device? %>
    <%= javascript_include_tag "https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js" %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

    <style>
      @import url('https://fonts.googleapis.com/css?family=Francois+One|Permanent+Marker');
    </style>

  </head>
  <body>
    <%= yield %>
  </body>
</html>
